CREATE TABLE bookmarks(
  local_id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  sura INTEGER,
  ayah INTEGER,
  page INTEGER,
  remote_id TEXT,
  last_updated INTEGER DEFAULT (strftime('%s', 'now')) NOT NULL
);

getBookmarks:
    SELECT * FROM bookmarks;

addBookmark:
    INSERT INTO bookmarks (sura, ayah, page, remote_id) VALUES (?, ?, ?, ?);